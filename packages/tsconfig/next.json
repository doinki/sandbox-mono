{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["DOM", "ESNext"],
    "noEmit": true
  },
  "exclude": [".next", ".yarn", "node_modules"]
}
